{
  "name": "nuxtjs-element-tailwind-starter",
  "description": "Repositório para desenvolvimento de aplicações web com o framework NestJS nas disciplinas de Engenharia de Software 1 e Projeto Integrador do câmpus Campo Mourão da UTFPR.",
  "version": "1.0.0",
  "author": {
    "name": "Gabriel Rocha"
  },
  "license": "MIT",
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "prepare": "husky install",
    "commit": "git-cz --disable-emoji",
    "lint": "yarn lint:js",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "dependencies": {
    "@nuxtjs/axios": "5.13.6",
    "@vitejs/plugin-vue2": "2.0.0",
    "core-js": "3.15.1",
    "element-ui": "2.15.10",
    "nuxt": "2.15.7",
    "vite": "3.1.7"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.14.7",
    "@fortawesome/free-brands-svg-icons": "6.2.0",
    "@fortawesome/free-solid-svg-icons": "6.2.0",
    "@nuxt/types": "~2.15.0",
    "@nuxtjs/eslint-config": "6.0.1",
    "@nuxtjs/eslint-module": "3.0.2",
    "@nuxtjs/fontawesome": "1.1.2",
    "@nuxtjs/tailwindcss": "4.2.0",
    "@vitest/coverage-c8": "^0.24.1",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-nuxt": "2.0.0",
    "eslint-plugin-tailwindcss": "3.6.2",
    "eslint-plugin-vue": "9.5.1",
    "git-cz": "4.9.0",
    "husky": "6.0.0",
    "jsdom": "^20.0.1",
    "lint-staged": "10.5.4",
    "postcss": "8.3.5",
    "prettier": "2.3.2",
    "vitest": "0.24.1"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  }
}
